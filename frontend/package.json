	"dependencies": {
		"@floating-ui/dom": "1.5.2",
		"@tanstack/svelte-query": "^4.35.3",
		"axios": "^1.5.0",
		"date-fns": "^2.30.0",
		"lodash": "^4.17.21",
		"pocketbase": "^0.18.0",
		"sveltekit-superforms": "^1.6.1",
		"zod": "^3.22.2"
	}
